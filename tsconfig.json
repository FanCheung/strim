{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "jsx": "react",
    "module": "commonjs",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "lib",
    "jsxFactory": "Snabbdom.createElement",
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "es5",
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ]
  },
  "exclude": [
    "lib",
    "node_modules",
    "test"
  ]
}